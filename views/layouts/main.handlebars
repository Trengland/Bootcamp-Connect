<!DOCTYPE html>
<html lang="en">
 <head>
   <meta charset="UTF-8">
   <meta http-equiv="X-UA-Compatible" content="IE=edge">
   <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css">
   <link rel="stylesheet" href="css/style.css">
   <title>{{ title }}</title>
 </head>
  <link rel="stylesheet" href="css/style.css">
 <body class="home">

   <div id="my-output">

   </div>
  
   <script id="my-template" type="text/x-handlebars-template">
   <header>
     <div id="title">
     </div>
     <div id="logo">
       <img src="{{ logotwo.png }}" />
     </div>
   </header>
   <div class="about">
     {{ aboutText }}
   </div>
   <main class="mainCenter">
     {{!-- main content here --}}
    {{{body}}}
   </main>
   <script src="https://cdn.jsdelivr.net/npm/handlebars@latest/dist/handlebars.js"></script>
   <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"></script>
   <script>
     const data = {
       title: "Homepage",
       logoSrc: "logotwo.png",
       aboutText: "Bootcamp Connect is a virtual site made to help you connect with your bootcamp classmates!"
     };


     const source = document.getElementById("my-template").innerHTML;
     const template = Handlebars.compile(source);
     const html = template(data);
     document.getElementById("my-output").innerHTML = html;
   </script>
 </body>
</html>
